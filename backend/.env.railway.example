# Railway Backend Environment Variables
# Copy these to your Railway backend service settings

# Django Settings
SECRET_KEY=your-secret-key-generate-new-one
DEBUG=False
ALLOWED_HOSTS=your-backend.railway.app,your-frontend.railway.app
CORS_ALLOWED_ORIGINS=https://your-frontend.railway.app

# Database (automatically provided by Railway PostgreSQL plugin)
DATABASE_URL=${DATABASE_URL}

# Redis (automatically provided by Railway Redis plugin)
REDIS_URL=${REDIS_URL}

# Python
PYTHON_VERSION=3.11.0
